<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.kvb">
<uses-permission android:name="android.permission.INTERNET" />
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
    >
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".register.RegisterActivity" />
        <activity android:name=".calculator.CalculatorActivity" />
        <activity android:name=".calculator.result.ResultActivity" />
        <activity android:name=".history.activity.HistoryActivity" />
        <activity android:name=".home.activity.HomeActivity" />
        <activity android:name=".info.activity.InfoActivity" />
        <activity android:name=".info.details.InfoDetailsActivity" />
    </application>

</manifest>